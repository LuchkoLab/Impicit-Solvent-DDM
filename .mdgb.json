    "arguments" : {
	"--solute" :
	{
	    "nargs" : "*",
	    "action" : "store",
	    "type" : {"<type>" : "str"},
	    "required" : true,
	    "help" : "Path and name of parm7, PDB or netCDF file for the simulation."
